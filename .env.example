# Environment Variables Template
# Copy this file to .env and update with your values

# Application Configuration
VITE_APP_TITLE=Community Help Hub
VITE_APP_DESCRIPTION=A social impact platform connecting people in need with local volunteers and NGOs

# Map Configuration
VITE_MAP_DEFAULT_LAT=40.7128
VITE_MAP_DEFAULT_LNG=-74.0060
VITE_MAP_DEFAULT_ZOOM=12

# API Configuration (for future backend integration)
# VITE_API_BASE_URL=https://api.communityhelpHub.org
# VITE_API_KEY=your_api_key_here

# Feature Flags
VITE_ENABLE_GEOLOCATION=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=false

# Development
VITE_DEV_MODE=true
